{
  "manifest_version": 2,

  "name": "Block",
  "description": "Checks how many times you have been on a set of websites and blocks if over setting",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "permissions": ["<all_urls>", "webRequest", "webRequestBlocking", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
